<h1>Editing Book</h1>
<%= form_for(@newbook) do |f| %>
    <h4>Title</h4>
    <%= f.text_field :title %>
    <h4>Body</h4>
    <%= f.text_area :body %>
    <%= f.submit 'Update Book' %>
<% end %>
<p><%=link_to "edit", edit_book_path(@newbook.id), class: "edit_#{@newbook.id}"  %>
   |
   <%=link_to "back", "/edit", class: "back" %>
</p>